- defaultAvatar = "https://cdn.gomix.com/0a15009e-17ee-4915-bc29-5ba03bb09517%2Favatar.svg"

section.features(role="navigation")
  h2 Featured Apps
  - console.log @featured()
  ul
    - @featured().forEach (project) ->
      - console.log project
      li= project.name
        a(href=@remixLink(project.url, project.id))
          span remix me

  p= @utils.searchhi
