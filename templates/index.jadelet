- Header = require "./includes/header"
- Featured = require "./includes/featured"
- CategoriesList = require "./includes/categories-list"
- ByFogcreek = require "./includes/by-fogcreek"
- Footer = require "./includes/footer"

- Category = require "../presenters/category"

span
  = Header(this)

  main(role="main")
    = Featured(this)

    - application = @
    - @selectedCategories().forEach (category) ->
      = Category(application, category)

    = CategoriesList(this)
    = ByFogcreek(this)

  = Footer(this)
  
  .beta beta
