  <body> 
    <script>
      var APP_URL = '<%= APP_URL %>';
      var API_URL = '<%= API_URL %>';
      var EDITOR_URL = '<%= EDITOR_URL %>';
      var CDN_URL = '<%= CDN_URL %>';
      var GITHUB_CLIENT_ID = '<%= GITHUB_CLIENT_ID %>';
      var FACEBOOK_CLIENT_ID = '<%= FACEBOOK_CLIENT_ID %>';
    </script>

    <noscript>
      <div class="content">
        <main>
          <h1><%= title %></h1>
          <p><%= description %></p>
        </main>
        <footer>
